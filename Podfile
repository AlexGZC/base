target 'App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for App
  pod 'Alamofire'
  pod 'Crashlytics'
  pod 'Fabric'
  pod 'FacebookLogin'
  pod 'NVActivityIndicatorView'
  pod 'SwiftyBeaver'
  pod 'TBEmptyDataSet'
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'



  target 'AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
